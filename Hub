}

local autoURL = AutoScripts[game.PlaceId]
if autoURL then
   Rayfield:Notify({
      Title = "Auto-Running Script",
      Content = "Detected supported game. Executing...",
      Duration = 5,
   })
   loadstring(game:HttpGet(autoURL))()
end
